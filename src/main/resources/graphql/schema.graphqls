type Query {
    latestOrders(page: Int, count: Int): [Order!]!
}


type Order {
    orderId: ID!,
    orderDate: String
    customer: Customer

}

type Customer {
    customerName: String,
    contactName: String,
    address: String,
    city: String,
    postalCode: String,
    country: String,
    customerId: ID!
}